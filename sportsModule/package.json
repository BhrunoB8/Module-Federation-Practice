{
  "name": "sports-module",
  "version": "0.0.0",
  "private": true,
  "description": "Description for Sports Module",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/",
    "cleanup": "rimraf target/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "serve": "npm run start --",
    "start": "npm run webapp:dev --",
    "start-tls": "npm run webapp:dev -- --env.tls",
    "pretest": "npm run lint",
    "test": "npm run vitest-run --",
    "test:watch": "npm run vitest",
    "vite-build": "vite build",
    "vite-serve": "vite",
    "vitest": "vitest",
    "vitest-run": "vitest --run --coverage",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev --",
    "webapp:build:dev": "npm run vite-build",
    "webapp:build:prod": "npm run vite-build",
    "webapp:dev": "npm run vite-serve",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod --",
    "webapp:serve": "npm run vite-serve",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/vue-fontawesome": "3.0.8",
    "@vue/compat": "3.5.13",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "13.0.0",
    "axios": "1.8.4",
    "bootstrap": "4.6.2",
    "bootstrap-vue": "2.23.1",
    "dayjs": "1.11.13",
    "pinia": "3.0.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "@module-federation/vite": "^1.2.6"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@pinia/testing": "1.0.0",
    "@tsconfig/node18": "18.2.4",
    "@types/node": "20.11.25",
    "@types/sinon": "17.0.4",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitest/coverage-v8": "3.0.9",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "axios-mock-adapter": "2.1.0",
    "concurrently": "9.1.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.5",
    "eslint-plugin-vue": "10.0.0",
    "flush-promises": "1.0.2",
    "generator-jhipster": "8.10.0",
    "happy-dom": "17.4.4",
    "numeral": "2.0.6",
    "postcss-import": "16.1.0",
    "postcss-url": "10.1.3",
    "prettier": "3.5.3",
    "prettier-plugin-packagejson": "2.5.10",
    "rimraf": "5.0.8",
    "sass": "1.64.2",
    "sinon": "20.0.0",
    "swagger-ui-dist": "5.20.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.28.0",
    "vite": "6.2.4",
    "vite-plugin-static-copy": "2.3.0",
    "vitest": "3.0.9",
    "vitest-sonar-reporter": "2.0.0",
    "wait-on": "8.0.3",
    "vite-plugin-federation": "^0.0.1"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
